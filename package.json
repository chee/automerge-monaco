  "keywords": [
    "automerge",
    "monaco",
    "editor",
    "collaborative",
    "crdt"
  ],
  "author": {
    "name": "chee"
  },
  "repository": {
    "url": "https://github.com/chee/automerge-monaco"
  },
  "version": "0.0.3",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/automonaco.cjs",
  "module": "./dist/automonaco.js",
