    "@automerge/automerge-repo-storage-indexeddb": "2.0.0-alpha.5",
    "@monaco-editor/loader": "^1.4.0",
    "monaco-editor": "^0.50.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-wasm": "^3.3.0"
  },
  "peerDependencies": {
    "@automerge/automerge-repo": "2.0.0-alpha.5",
